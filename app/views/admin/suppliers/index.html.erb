<h1>Listing admin_suppliers</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Phone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_suppliers.each do |admin_supplier| %>
  <tr>
    <td><%= admin_supplier.name %></td>
    <td><%= admin_supplier.address %></td>
    <td><%= admin_supplier.phone %></td>
    <td><%= link_to 'Show', admin_supplier %></td>
    <td><%= link_to 'Edit', edit_admin_supplier_path(admin_supplier) %></td>
    <td><%= link_to 'Destroy', admin_supplier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

