<h1>Listing admin_products</h1>

<table>
  <tr>
    <th>Catalog</th>
    <th>Unit</th>
    <th>Supplier</th>
    <th>Name</th>
    <th>Ref</th>
    <th>Manufacturer</th>
    <th>Location</th>
    <th>Price</th>
    <th>Amount</th>
    <th>Status</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_products.each do |admin_product| %>
  <tr>
    <td><%= admin_product.catalog %></td>
    <td><%= admin_product.unit %></td>
    <td><%= admin_product.supplier %></td>
    <td><%= admin_product.name %></td>
    <td><%= admin_product.ref %></td>
    <td><%= admin_product.manufacturer %></td>
    <td><%= admin_product.location %></td>
    <td><%= admin_product.price %></td>
    <td><%= admin_product.amount %></td>
    <td><%= admin_product.status %></td>
    <td><%= admin_product.description %></td>
    <td><%= link_to 'Show', admin_product %></td>
    <td><%= link_to 'Edit', edit_admin_product_path(admin_product) %></td>
    <td><%= link_to 'Destroy', admin_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Product', new_admin_product_path %>
